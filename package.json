  "private": true,
  "dependencies": {
    "concurrently": "^5.0.2",
    "express": "^4.17.1",
    "http-proxy-middleware": "^3.0.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.9.1",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
